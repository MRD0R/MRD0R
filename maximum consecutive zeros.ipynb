    "def solution(n): \n",
    "    if n==0 or n&(n-1)==0 :\n",
    "        return 0\n",
    "    b = ''\n",
    "    while n>0:\n",
    "        b = str(n%2) + b\n",
    "        n = n // 2\n",
    "    return(max(len(i) for i in b.split('1')))\n",
    "N = int(input())\n",
    "solution(N)"
   ]
